[1m+++ b/ServicesQLSV/src/main/java/com/itplus/servicesqlsv/Model/DBPool.java[m
[36m@@ -0,0 +1,185 @@[m
[32m+[m[32m/*[m
[32m+[m[32m * To change this license header, choose License Headers in Project Properties.[m
[32m+[m[32m * To change this template file, choose Tools | Templates[m
[32m+[m[32m * and open the template in the editor.[m
[32m+[m[32m */[m
[32m+[m[32mpackage com.itplus.servicesqlsv.Model;[m
[32m+[m
[32m+[m[32mimport java.sql.CallableStatement;[m
[32m+[m[32mimport java.sql.Connection;[m
[32m+[m[32mimport java.sql.DriverManager;[m
[32m+[m[32mimport java.sql.PreparedStatement;[m
[32m+[m[32mimport java.sql.ResultSet;[m
[32m+[m[32mimport java.sql.SQLException;[m
[32m+[m[32mimport java.sql.Statement;[m
[32m+[m[32mimport java.util.Iterator;[m
[32m+[m[32mimport java.util.LinkedList;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @author Dung NT[m
[32m+[m[32m */[m
[32m+[m[32mpublic class DBPool {[m
[32m+[m[32m    private static Logger logger = new Logger("DBPool");[m
[32m+[m[32m    private static LinkedList pool = new LinkedList();[m
[32m+[m[32m    public final static int MAX_CONNECTIONS = 10;[m
[32m+[m[32m    public final static int INI_CONNECTIONS = 2;[m
[32m+[m
[32m+[m[32m    public DBPool() {[m
[32m+[m[32m        build(INI_CONNECTIONS);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static Connection makeDBConnection() throws SQLException {[m
[32m+[m[32m        Connection conn = null;[m
[32m+[m[32m        try {[m
[32m+[m[32m            Class.forName("com.microsoft.sqlserver.jdbc.SQLServerDriver");[m
[32m+[m[32m            String dbURL = "jdbc:sqlserver://localhost\\TIENDUNG;databaseName=QLSV";[m
[32m+[m[32m            String user = "sa";[m
[32m+[m[32m            String pass = "tiendung";[m
[32m+[m[32m            conn = DriverManager.getConnection(dbURL, user, pass);[m
[32m+[m[32m        } catch (Exception e) {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m        return conn;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void build(int number) {[m
[32m+[m[32m        logger.log("Establishing " + number + " connections...");[m
[32m+[m[32m        Connection conn = null;[m
[32m+[m[32m        release();[m
[32m+[m[32m        for (int i = 0; i < number; i++) {[m
[32m+[m[32m            try {[m
[32m+[m[32m                conn = makeDBConnection();[m
[32m+[m[32m            } catch (SQLException ex) {[m
[32m+[m[32m                logger.log("Error: " + ex.getMessage());[m
[32m+[m[32m            }[m
[32m+[m[32m            if (conn != null) {[m
[32m+[m[32m                pool.addLast(conn);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        logger.log("Number of connection: " + number);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static Connection getConnection() {[m
[32m+[m[32m        Connection conn = null;[m
[32m+[m[32m        try {[m
[32m+[m[32m            synchronized (pool) {[m
[32m+[m[32m                conn = (java.sql.Connection) pool.removeFirst();[m
[32m+[m[32m            }[m
[32m+[m[32m            if (conn == null) {[m
[32m+[m[32m                conn = makeDBConnection();[m
[32m+[m[32m            }[m
[32m+[m[32m            try {[m
[32m+[m[32m                conn.setAutoCommit(true);[m
[32m+[m[32m            } catch (Exception ex) {[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        } catch (Exception e) {[m
[32m+[m[32m            logger.error("Method getConnection(): Error executing >>>" + e.toString());[m
[32m+[m[32m            try {[m
[32m+[m[32m                logger.log("Make connection again.");[m
[32m+[m[32m                conn = makeDBConnection();[m
[32m+[m[32m                conn.setAutoCommit(true);[m
[32m+[m[32m            } catch (SQLException e1) {[m
[32m+[m[32m            }[m
[32m+[m[32m            logger.log("" + conn);[m
[32m+[m[32m        }[m
[32m+[m[32m        return conn;[m
[32m+[m[32m    }[m
[32m+[m[41m   [m
[32m+[m
[32m+[m[32m    public static void putConnection(java.sql.Connection conn) {[m
[32m+[m[32m        try {[m[41m [m
[32m+[m[32m            if (conn == null || conn.isClosed()) {[m
[32m+[m[32m                logger.log("putConnection: conn is null or closed: " + conn);[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (pool.size() >= MAX_CONNECTIONS) {[m
[32m+[m[32m                conn.close();[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (SQLException ex) {[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        synchronized (pool) {[m
[32m+[m[32m            pool.addLast(conn);[m
[32m+[m[32m            pool.notify();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    public static void release() {[m
[32m+[m[32m        logger.log("Closing connections in pool...");[m
[32m+[m[32m        synchronized (pool) {[m
[32m+[m[32m            for (Iterator it = pool.iterator(); it.hasNext();) {[m
[32m+[m[32m                Connection conn = (Connection) it.next();[m
[32m+[m[32m                try {[m
[32m+[m[32m                    conn.close();[m
[32m+[m[32m                } catch (SQLException e) {[m
[32m+[m[32m                    logger.error([m
[32m+[m[32m                            "release: Cannot close connection! (maybe closed?)");[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            pool.clear();[m
[32m+[m[32m        }[m
[32m+[m[32m        logger.log("Release connection OK");[m
[32m+[m[32m    }[m
[32m+[m[41m [m
[32m+[m
[32m+[m[32m    public static void releaseConnection(Connection conn, PreparedStatement preStmt) {[m
[32m+[m[32m        putConnection(conn);[m
[32m+[m[32m        try {[m
[32m+[m[32m            if (preStmt != null) {[m
[32m+[m[32m                preStmt.close();[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (SQLException e) {[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    public static void releaseConnection(Connection conn, PreparedStatement preStmt, ResultSet rs) {[m
[32m+[m[32m        releaseConnection(conn, preStmt);[m
[32m+[m[32m        try {[m
[32m+[m[32m            if (rs != null) {[m
[32m+[m[32m                rs.close();[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (SQLException e) {[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void releaseConnection(Connection conn, Statement Stmt, ResultSet rs) {[m
[32m+[m[32m        putConnection(conn);[m
[32m+[m[32m        try {[m
[32m+[m[32m            if (Stmt != null) {[m
[32m+[m[32m                Stmt.close();[m
[32m+[m[32m            }[m
[32m+[m[32m            if (rs != null) {[m
[32m+[m[32m                rs.close();[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (SQLException e) {[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void releaseConnection(Connection conn, PreparedStatement preStmt, Statement stmt, ResultSet rs) {[m
[32m+[m[32m        releaseConnection(conn, preStmt, rs);[m
[32m+[m[32m        try {[m
[32m+[m[32m            if (stmt != null) {[m
[32m+[m[32m                stmt.close();[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (SQLException e) {[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void releaseConnection(Connection conn, CallableStatement cs, ResultSet rs) {[m
[32m+[m[32m        putConnection(conn);[m
[32m+[m[32m        try {[m
[32m+[m[32m            if (cs != null) {[m
[32m+[m[32m                cs.close();[m
[32m+[m[32m            }[m
[32m+[m[32m            if (rs != null) {[m
[32m+[m[32m                rs.close();[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (SQLException e) {[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
